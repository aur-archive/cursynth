+    Read,                           \
+    Write,                          \
+    Flush,                          \
+    Close,                          \
+    QuerySecurity,                      \
+    SetSecurity,                        \
+    FastDeviceIoControl,                    \
+    FastRead,                       \
+    FastWrite,                      \
+  }
