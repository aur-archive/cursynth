diff -Naur a/rtaudio/include/asiodrvr.h b/rtaudio/include/asiodrvr.h
--- a/rtaudio/include/asiodrvr.h	1970-01-01 01:00:00.000000000 +0100
+++ b/rtaudio/include/asiodrvr.h	2014-12-30 22:23:30.569518457 +0100
