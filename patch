+ # if !defined(DEFINE_WAVEFORMATEX_GUID)
+  #  define DEFINE_WAVEFORMATEX_GUID(x)                 \
+  (USHORT)(x), 0x0000, 0x0010, { 0x80, 0x00, 0x00, 0xaa, 0x00, 0x38, 0x9b, 0x71 }
+ # endif
